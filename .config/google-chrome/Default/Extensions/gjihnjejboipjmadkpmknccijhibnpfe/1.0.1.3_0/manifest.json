{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icons/icon_19_19.png",
      "default_popup": "popup.html",
      "default_title": "Downloads"
   },
   "content_scripts": [ {
      "js": [ "gizmo.js" ],
      "matches": [ "\u003Call_urls>" ]
   }, {
      "all_frames": true,
      "exclude_globs": [  ],
      "js": [ "support/ext.js", "support/support.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Manage your downloads directly from your browser's toolbar. Forget the fat download shelf for good.",
   "icons": {
      "48": "icons/icon-48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "30.0.0.0",
   "name": "Downloads - Your Download Box",
   "permissions": [ "tabs", "downloads", "downloads.open", "downloads.shelf", "storage", "notifications", "http://*/*", "https://*/*", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1.3",
   "web_accessible_resources": [ "icons/file_down.png", "icons/file_down6.png", "support/pages/adoptions.html", "support/pages/privacy_policy.html" ]
}
